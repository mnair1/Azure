{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": "iam:PassRole",
            "Resource": "*"
        },
        {
            "Sid": "IAMDeny",
            "Action": [
                "iam:ChangePassword",
                "iam:CreateSAMLProvider",
                "iam:DeactivateMFADevice",
                "iam:DeleteAccountPasswordPolicy",
                "iam:DeleteSAMLProvider",
                "iam:DeleteUser",
                "iam:UpdateSAMLProvider",
                "iam:AttachUserPolicy",
                "iam:AttachGroupPolicy",
                "iam:PutGroupPolicy",
                "macie:Disassociate*"
            ],
            "Resource": "*",
            "Effect": "Deny"
        }
    ]
}